                ;*****************************************************************
                ; Constants
                ;*****************************************************************
                
                ; =================== Key Pad Constants ===================
                
 0050           NoNumberPressed EQU 50H ; The Key-Number if no number is pressed
 0000           BallKeyPress EQU 00H ; The Key-Number if Ball is pressed
 0001           StrikeKeyPress EQU 01H ; The Key-Number if Strike is pressed
 0002           OutKeyPress EQU 02H ; The Key-Number if Strike is pressed
                
                ; ================== Dot Matrix Constants ==================
                
 0010           DotOnDelay1 EQU 010H ; The wait time while the Dot Matrix is On
 0010           DotOnDelay2 EQU 010H ; The wait time while the Dot Matrix is On
                
 0060           BallOnDelay1 EQU 060H ; The wait time while the Ball is Displayed
 0060           BallOnDelay2 EQU 060H ; The wait time while the Ball is Displayed
                
 0004           InitPitchRow EQU 4 ; The initial row of the baseball on the field
 0010           InitPitchBits EQU 00010000B ; The initial bit pattern on the field 
 0002           InitBallRow EQU 2 ; The initial row of a ball on Home Plate
 0001           InitBallBits EQU 00000001B ; The initial bit pattern on Home Plate
 0000           InitStrikeRow EQU 0 ; The initial row of a strike on Home Plate
 0001           InitStrikeBits EQU 00000001B ; The initial bit pattern on Home Plate
 0008           BallLastRow EQU 8 ; The final Row of the ball after throwing it
                
 0002           Base1Row EQU 2 ; The row number of first base
 0040           Base1Bits EQU 01000000B  ; The bit pattern of first base
 0002           Base2Row EQU 2 ; The row number of second base
 0004           Base2Bits EQU 00000100B  ; The bit pattern of second base
 0006           Base3Row EQU 6 ; The row number of third base
 0004           Base3Bits EQU 00000100B  ; The bit pattern of third base
                
                ; ========================= LCD Constants ========================= 
                
 0000           Line1-Address           EQU     00H  ; Address in LCD DDRAM memory where Line 1 begins
 0040           Line2-Address           EQU     40H  ; Address in LCD DDRAM memory where Line 2 begins
 0008           Font-Row-Max            EQU     08H  ; The Number of Rows in a Character Font
                
                ;*****************************************************************
                ; Device Registers
                ;*****************************************************************
                
                ; ============== Memory Locations of LCD Registers ==============
                
 FFE0           LCD-Write-Instr        EQU     0FFE0H     ; The register in the LCD which receives instructions
 FFE1           LCD-Write-Data         EQU     0FFE1H     ; The register in the LCD which receives data
                
 FFE2           LCD-Read-Instr         EQU     0FFE2H     ; The register in the LCD which sends instructions
 FFE3           LCD-Read-Data          EQU     0FFE3H     ; The register in the LCD which sends data
                
                
                ; ===== Memory Locations of the Dot Matrix Rows and Columns =====
                
 FFC5           DotMatrix-Green        EQU     0FFC5H     ; Which columns are colored green
 FFC6           DotMatrix-Red          EQU     0FFC6H     ; Which columns are colored red
 FFC7           DotMatrix-Row          EQU     0FFC7H     ; Which rows are turned on
                
                ; ========= Memory Locations of KeyPad Rows and Columns =========
                
 FFF0           Keypad-Data-Out        EQU     0FFF0H    ; Write which columns to consider
 FFF1           Keypad-Data-In         EQU     0FFF1H     ; Read the rows 
                
                ; ====== Memory Locations of LED 7-Segment Array Registers ======
                
 FFC3           LED-7-Segment1         EQU     0FFC3H    ; The first and second LED Numbers
 FFC2           LED-7-Segment2         EQU     0FFC2H    ; The third and fourth LED Numbers
 FFC1           LED-7-Segment3         EQU     0FFC1H    ; The fifth and sixth LED Numbers
                
 FFEF           Buzzer		       EQU     0FFEFH
                
                ; ========== Memory Location of Seven Segment Display ==========
                
 FFC0           Seven-Segment 	       EQU     0FFC0H    ; The location of the 7-segment display
                
                ;*****************************************************************
                ; LCD Instructions
                ; Instruction Codes for each of the LCD Commands
                ;*****************************************************************
                
 0001           Clear-LCD              EQU     01H   ; Clear the LCD Screen entirely
 0002           Cursor-Home-LCD        EQU     02H   ; Move the Cursor to the Home position
                
                ; Does the cursor address increase or decrease after printing a character?
                ; and does the screen shift?
 0004           Cur-Dec-LCD            EQU     04H   ; Move the Cursor Address Left after writing
 0005           Cur-DecShift_LCD       EQU     05H   ; Move the Cursor Address Left and Shift the Screen after writing
 0006           Cur-Inc-LCD            EQU     06H   ; Move the Cursor Address Right after writing
 0007           Cur-IncShift_LCD       EQU     07H   ; Move the Cursor Address Right and Shift the Screen after writing
                
                ; Turn on or off the Display, the Cursor, and the Blinking Cursor
 0008           DCB-Off-LCD            EQU     08H    ;Display(OFF) ,Cursor(OFF) ,Blink(OFF)
 0009           Blink-On-LCD           EQU     09H    ;Display(OFF) ,Cursor(OFF) ,Blink(ON)
 000A           Cursor-On-LCD          EQU     0AH    ;Display(OFF) ,Cursor(ON) ,Blink(OFF)
 000B           CurBlink-On-LCD        EQU     0BH    ;Display(OFF) ,Cursor(ON) ,Blink(ON)
 000C           Display-On-LCD         EQU     0CH    ;Display(ON) ,Cursor(OFF) ,Blink(OFF)
 000D           DispBlink-On-LCD       EQU     0DH    ;Display(ON) ,Cursor(OFF) ,Blink(ON)
 000E           DispCur-On-LCD         EQU     0EH    ;Display(ON) ,Cursor(ON) ,Blink(OFF)
 000F           DCB-On-LCD             EQU     0FH    ;Display(ON) ,Cursor(ON) ,Blink(ON)
                
                ;Set the number of Rows and the Font Size
 0030           OneRow_5X7-LCD         EQU     30H    ;8-Bit 1 Row 5*7  , 1/16 duty
 0034           OneRow_5X10-LCD        EQU     34H    ;8-Bit 1 Row 5*10 , 1/16 duty
 0038           TwoRows-5X7-LCD        EQU     38H    ;8-Bit 2 Row 5*7  , 1/16 duty
 003C           TwoRows-5X10-LCD       EQU     3CH    ;8-Bit 2 Row 5*10 , 1/16 duty
                
                ;Set the CG RAM Address
 0040           Start-CGRAM-LCD        EQU     40H    ;0 100 0000 : CG-RAM Address Setting
                
                ;Set the DD RAM Address, which moves the cursor to that address
 0080           Move-Cursor-LCD        EQU     80H    ;1 000 0000 : Data Entry Start Address
                
                
                
